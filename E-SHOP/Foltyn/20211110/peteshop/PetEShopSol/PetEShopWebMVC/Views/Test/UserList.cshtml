@model PetEShopWebMVC.Models.UserListModel

@{
    ViewData["Title"] = "List of users";
    Layout = "~/Views/Shared/_Layout.cshtml";
}


<h1>List of users</h1>

<table class="table">
    <tr>
        <th>ID</th>
        <th>Username</th>
        <th>Email</th>
    </tr>
@foreach (var user in Model.Users)
{
    <tr>
        <td>@user.ID</td>
        <td>@user.Username</td>
        <td>@user.Email</td>
    </tr>
}
</table>
