@model PetEShopWebMVC.Models.CarModel

@{
    ViewData["Title"] = "Car";
    @* Layout = "~/Views/Shared/_Layout.cshtml"; *@
}

<h1>Car</h1>

<h2>View example where we have to explicitly specify the name of the view</h2>

<table>
    <tr>
        <th>Car brand</th>
        <th>Model</th>
    </tr>
@foreach (var car in Model.Cars)
{
    <tr>
        <td>@car.Brand</td>
        <td>@car.Model</td>
    </tr>
}
</table>

