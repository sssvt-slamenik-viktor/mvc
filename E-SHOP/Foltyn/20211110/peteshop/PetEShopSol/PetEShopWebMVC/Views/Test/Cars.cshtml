@model PetEShopWebMVC.Models.CarModel

@{
    ViewData["Title"] = "Car";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>Car</h1>

<h2>View example "by convention"</h2>


<table class="table">
    <thead>
        <tr>
            <th>Car brand</th>
            <th>Model</th>
        </tr>
    </thead>
    <tbody>
@foreach (var car in Model.Cars)
{
        <tr>
            <td>@car.Brand</td>
            <td>@car.Model</td>
        </tr>
}
    </tbody>
</table>

