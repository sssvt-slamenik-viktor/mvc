@model PetEShopWebMVC.Models.UsersListModel

@{
    ViewData["Title"] = "List of users";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1>List of users</h1>


<table class="table">
    <tr>
        <th>Id</th>
        <th>Username</th>
        <th>E-mail</th>
    </tr>
@foreach (var user in Model.Users)
{
    <tr>
        <td>@user.ID</td>
        <td><a href="/test/load-user/@user.ID">@user.Username</a></td>
        <td>@user.Email</td>
    </tr>
}
</table>